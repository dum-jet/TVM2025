Arithmetic {
	Expr 
    	= AddExpr
      
    AddExpr 
     	= (MulExpr ("+" | "-"))* MulExpr
        
    MulExpr
    	= (Atom ("*" | "/"))* Atom
        
    Atom
        = "(" Expr ")" -- paren
        | "-" Atom --neg
        | number
        | variable
	
	variable = letter alnum*

    number
      = digit+ 
}